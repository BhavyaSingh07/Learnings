@model IEnumerable<string>
@inject ICitiesService citiesServiceInview
@{
	ViewBag.Title = "Home";
}

<h1>Home</h1>
@{
	List<string> citiesFromServiceInview = citiesServiceInview.GetCities();
}
<ul class="list">
	@foreach (string city in citiesFromServiceInview)
	{
	<li>@city</li> 
}
</ul>

<div>
	@ViewBag.InstanceId_CitiesService_1
</div>
<div>
	@ViewBag.InstanceId_CitiesService_2
</div>
<div>
	@ViewBag.InstanceId_CitiesService_3
</div>
<div class="box">
	@ViewBag.InstanceId_CitiesService_InScope
</div>
